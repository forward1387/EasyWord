{
  "name": "EasyWord",
  "version": "0.0.1",
  "private": true,
  "description": "Web Application for easy study a word.",

  "dependencies": {
    "async": "latest",
    "body-parser": "latest",
    "cookie-parser": "latest",
    "crypto": "latest",
    "debug": "latest",
    "errorhandler": "latest",
    "express": "latest",
    "mongoose": "latest",
    "morgan": "latest",
    "nconf": "latest",
    "q": "latest",
    "winston": "latest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/forward1387/EasyWord.git"
  },
  "author": "Yuriy Oprysk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/forward1387/EasyWord/issues"
  },
  "scripts": {
    "postinstall": "bower install --allow-root",

    "prestart": "npm install",
    "start": "node ./bin/www",

    "pretest": "npm install",
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js  --single-run",

    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",

    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e-tests/protractor.conf.js",

    "update-index-async": "node -e \"require('shelljs/global'); sed('-i', /\\/\\/@@NG_LOADER_START@@[\\s\\S]*\\/\\/@@NG_LOADER_END@@/, '//@@NG_LOADER_START@@\\n' + sed(/sourceMappingURL=angular-loader.min.js.map/,'sourceMappingURL=bower_components/angular-loader/angular-loader.min.js.map','app/bower_components/angular-loader/angular-loader.min.js') + '\\n//@@NG_LOADER_END@@', 'app/index-async.html');\""
  },
  "homepage": "https://github.com/forward1387/EasyWord",
  "devDependencies": {
    "karma": "latest",
    "protractor": "latest",
    "http-server": "latest",
    "bower": "latest",
    "shelljs": "latest",
    "karma-junit-reporter": "latest"
  }
}
